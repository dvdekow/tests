<h2>Detail Bought</h2>

<div id="algo1">
  <table border="3">
    <tr>
      <th width="20%">Product ID</th>
      <th width="20%">User ID</th>
      <th width="20%">Boughted At</th>
    </tr>
    <% unless @allbought.nil? %>
      <% @allbought.each do |b| %>
        <tr>
          <td><%= @allbought.product %></td>
          <td><%= @allbought.user %></td>
          <td><%= @allbought.created_at %></td>
        </tr>
      <% end %>
    <% end %>
  </table>
</div>
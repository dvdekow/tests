<h2>Detail Clicked</h2>

<div id="algo1">
  <table border="3">
    <tr>
      <th width="20%">Product ID</th>
      <th width="20%">User ID</th>
      <th width="20%">Clicked At</th>
    </tr>
    <% unless @allclick.nil? %>
      <% @allclick.each do |c| %>
        <tr>
          <td><%= c.product %></td>
          <td><%= c.user %></td>
          <td><%= c.created_at %></td>
        </tr>
      <% end %>
    <% end %>
  </table>
</div>

<h3>Most Clicked</h3>